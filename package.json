{
  "name": "chess-maine-event-scraper",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Jeff Merckens",
  "main": "dist/index.js",
  "module": "dist/chess-maine-event-scraper.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "node dist/index.js",
    "start:dev": "dts watch",
    "test": "dts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/chess-maine-event-scraper.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/chess-maine-event-scraper.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.0.1",
    "@tsconfig/recommended": "^1.0.1",
    "@types/cheerio": "^0.22.31",
    "@types/node": "10.9.1",
    "@types/node-cron": "^3.0.2",
    "dts-cli": "^1.6.0",
    "husky": "^8.0.1",
    "size-limit": "^8.0.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.12",
    "node-cron": "^3.0.2"
  }
}
